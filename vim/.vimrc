set nocompatible
"set number
syntax on
set showcmd
set showmode
set mouse=a
set clipboard=unnamedplus
set scrolloff=5

set tabstop=4
set shiftwidth=4
set expandtab
set autoindent
set smartindent

set ignorecase
set smartcase
set incsearch
set hlsearch


nnoremap <SPACE> :nohlsearch<CR>

set hidden

filetype on
filetype indent on
filetype plugin on

